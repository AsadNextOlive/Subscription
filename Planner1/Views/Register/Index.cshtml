@model List<Planner1.Models.Register>

@{
    ViewData["Title"] = "Index";
}

<h1>List of Registered</h1>
<table class="table table-info table-striped">
    <thead>
        <tr>
            <td class="fw-bold text-uppercase">Id</td>
            <td class="fw-bold text-uppercase">Username</td>
            <td class="fw-bold text-uppercase">Password</td>
            <td class="fw-bold text-uppercase">Email</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Username</td>
                <td>@item.Password</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>